{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && cd apps/pdf-viewer && npm ci --no-audit --no-fund && npm run build && cd .. && npm run postbuild",
    "postbuild": "node scripts/copy-viewer-assets.cjs",
    "vercel-build": "npm ci --legacy-peer-deps || npm install --legacy-peer-deps && npm run build",
    "preview": "vite preview",
    "test": "vitest --run src/tests",
    "test:ui": "vitest src/tests",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc -p tsconfig.json --noEmit || true",
    "e2e": "playwright test",
    "e2e:smoke": "playwright test smoke.spec.ts",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "prepare": "husky install",
    "postinstall": "node scripts/copy-pdf-worker.cjs || true",
    "check:pdf-imports": "grep -R 'react-pdf\\|pdfjs-dist\\|react-pdf-highlighter' src | grep -v 'PdfHighlighter.tsx' && (echo '\n❌ Encontrado import estático de PDF' && exit 1) || echo 'OK'",
    "vercel:preflight": "node -e \"const fs=require('fs'); if(fs.existsSync('../vercel.json')){console.error('❌ Remova vercel.json da raiz: use apenas frontend/vercel.json'); process.exit(1);} console.log('✅ vercel.json OK apenas no frontend');\"",
    "validate-routes": "node scripts/validate-routes.cjs",
    "seed:dev": "tsx scripts/seed-dev.ts --verbose",
    "seed:dev:backend": "tsx scripts/seed-dev.ts --backend --verbose",
    "seed:dev:clear": "tsx scripts/seed-dev.ts --clear --verbose",
    "check-all": "npm run lint && npm test && npm run e2e && npm run validate-routes && npm run check:pdf-imports && npm run vercel:preflight",
    "check-all:ci": "npm run lint && npm test && npm run e2e --reporter=json && npm run validate-routes && npm run check:pdf-imports && npm run vercel:preflight",
    "sw:status": "node -e \"console.log('Set VITE_ENABLE_SW=1 para registrar SW; VITE_DISABLE_SW=1 para desinstalar em clientes.')\""
  },
  "dependencies": {
    "axios": "^1.11.0",
    "framer-motion": "^12.23.12",
    "papaparse": "^5.5.3",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "4.7.76",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^4.12.0",
    "react-pdf": "7.7.3",
    "react-pdf-highlighter": "7.0.0",
    "react-router-dom": "^7.8.1",
    "react-toastify": "^11.0.5",
    "web-vitals": "^5.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.0.5",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^24.1.3",
    "lint-staged": "^16.1.6",
    "madge": "^8.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
