{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run check:pdf-imports && node -e \"(require('fs').existsSync('public/pdf.worker.min.js')||require('fs').existsSync('public/pdf.worker.min.mjs'))||process.exit(2)\" && echo worker OK",
    "build": "vite build",
    "postbuild": "node scripts/guard-no-eager-pdf-konva.cjs && node scripts/verify-manifest-no-pdf-konva.cjs",
    "build:all": "npm run build && npm run build:viewer",
    "build:viewer": "node apps/pdf-viewer/scripts/copy-worker.cjs && vite build --config apps/pdf-viewer/vite.config.ts --outDir dist/viewer",
    "vercel-build": "npm ci --legacy-peer-deps || npm install --legacy-peer-deps && npm run build",
    "preview": "vite preview",
    "test": "jest --passWithNoTests",
    "lint": "eslint src --max-warnings=0",
    "postinstall": "node ./scripts/copy-pdf-worker.cjs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "check:pdf-imports": "node ./scripts/check-static-pdf-imports.cjs",
    "check:entry-has-no-pdf-konva": "node ./scripts/check-entry-html-clean.cjs",
    "check:dist:pdf-leak": "bash ./scripts/check-dist-pdf-leak.sh",
    "relink:pdf-annotator": "node ./scripts/relink-pdf-imports.cjs",
    "smoke:pdf": "node ./scripts/smoke-pdf.cjs --essay $ESSAY_ID || node ./scripts/smoke-pdf.cjs --url $SMOKE_PDF_URL",
    "guard:manifest": "node ./scripts/verify-manifest-no-pdf-konva.cjs"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "framer-motion": "^12.23.12",
    "konva": "^10.0.2",
    "nanoid": "^5.1.5",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "4.7.76",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^4.12.0",
    "react-konva": "^18.2.14",
    "react-pdf": "^10.1.0",
    "react-pdf-highlighter": "7.0.0",
    "react-router-dom": "^7.8.1",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.2.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "madge": "^8.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.2"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "engines": {
    "node": "20.x"
  }
}
