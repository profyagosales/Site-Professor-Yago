{
  "framework": "vite",
  "buildCommand": "npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://api.professoryagosales.com.br/:path*" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "routes": [
    { "src": "/pdf.worker.mjs", "dest": "/pdf.worker.mjs" },
    { "src": "/viewer/(.*)", "dest": "/viewer/$1" }
  ]
}
