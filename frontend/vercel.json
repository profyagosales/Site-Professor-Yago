{
  "framework": "vite",
  "buildCommand": "npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://api.professoryagosales.com.br/api/$1" },
    { "source": "/pdf.worker.mjs", "destination": "/pdf.worker.mjs" },
    { "source": "/viewer/(.*)", "destination": "/viewer/$1" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
