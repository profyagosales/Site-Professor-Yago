{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://api.professoryagosales.com.br/api/$1" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "redirects": [
    { "source": "/professor/login", "destination": "/login-professor", "permanent": true },
    { "source": "/aluno/login", "destination": "/login-aluno", "permanent": true }
  ]
}
